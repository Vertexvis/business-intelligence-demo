    "react": "^17",
    "react-dom": "^17",
    "react-dropzone": "^11.3",
    "tailwindcss": "^2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@types/node": "^14.14",
    "@types/papaparse": "^5.2",
    "@types/react": "^17.0",
    "@vertexvis/eslint-config-vertexvis-typescript": "^0.4",
    "@vertexvis/typescript-config-vertexvis": "^1.1",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.22",
    "postcss": "^8.2",
